scalar JSON

type Run {
    runId: ID!
    runName: String
    startTime: String
    state: String
    success: Boolean
    exitStatus: Int
    errorReport: String
    completeTime: String
    commandLine: String
    engineParameters: EngineParameters
    parameters: JSON
}

type EngineParameters {
    launchDir: String
    projectDir: String
    resume: String
    revision: String
    workDir: String
}

type Query {
    runs(runId: String, runName: String, state: String): [Run]
}
